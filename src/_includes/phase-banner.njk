{% if meta.env == 'production' %}

  {% set tag = 'Beta' %}

{% elif meta.env == 'prototype' %}

  {% set tag = 'Prototype' %}

{% else %}

  {% set tag = 'Development' %}
  
{% endif %}

<div class="govuk-width-container">
  {{ component('phase-banner', {
    tag: {
      text: tag
    },
    html: 'This is a new service. If you have questions or suggestions, <a class="govuk-link govuk-link govuk-link--no-visited-state" href="' + '/send-your-feedback' | url + '">send us your feedback</a>.'
  }) }}
</div>
